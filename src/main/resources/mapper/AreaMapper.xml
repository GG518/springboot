<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wd.springboot.springbootshoping.dao.AreaDao">
    <select id="queryArea" resultType="com.wd.springboot.springbootshoping.entity.Area">
        select
        area_id,
        area_name,
        priority,
        create_time,
        update_time
        from tb_area ORDER by priority DESC
    </select>
</mapper>